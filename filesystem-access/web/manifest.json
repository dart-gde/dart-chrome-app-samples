{
  "name": "Filesystem Access Sample",
  "version": "1",
  
  "manifest_version": 2,
  "minimum_chrome_version": "31",  
  
  "icons": {"128": "dart_icon.png"},
  
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  
  "permissions": [
    { "fileSystem": ["write", "retainEntries", "directory"] },
    "storage"
  ],
  
  "file_handlers": {
    "text": {
      "types": [
          "text/*"
      ],
      "title": "Text editor"
    }
  }
}
